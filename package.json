{
  "name": "fanger",
  "version": "0.2.4",
  "description": "Defang and refang IoC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts"
  },
  "bin": {
    "defang": "dist/bin/defang.js",
    "refang": "dist/bin/refang.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninoseki/fanger.git"
  },
  "author": "Manabu Niseki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninoseki/fanger/issues"
  },
  "homepage": "https://github.com/ninoseki/fanger#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.4",
    "@types/tlds": "^1.199.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0",
    "get-stdin": "^8.0.0",
    "ioc-extractor": "^2.3.4"
  }
}
