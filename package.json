{
  "name": "fanger",
  "version": "0.2.6",
  "description": "Defang and refang IoC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts"
  },
  "bin": {
    "defang": "dist/bin/defang.js",
    "refang": "dist/bin/refang.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninoseki/fanger.git"
  },
  "author": "Manabu Niseki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninoseki/fanger/issues"
  },
  "homepage": "https://github.com/ninoseki/fanger#readme",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0",
    "get-stdin": "^8.0.0",
    "ioc-extractor": "^2.4.0"
  }
}
